{
  "storages": [
    {
      "name": "local",
      "type": "local",
      "config": {
        "rootPath": "./versions"
      }
    },
    {
      "name": "minio",
      "type": "minio",
      "config": {
        "endPoint": "{{ MINIO_ENDPOINT }}",
        "port": 443,
        "useSSL": true,
        "accessKey": "{{ MINIO_ACCESS_KEY }}",
        "secretKey": "{{ MINIO_SECRET_KEY }}",
        "bucketName": "{{ MINIO_BUCKET_NAME }}",
        "prefix": "{{ MINIO_PREFIX }}"
      }
    }
  ]
}
